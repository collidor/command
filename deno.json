{
    "name": "@collidor/command",
    "version": "5.1.3",
    "license": "MIT",
    "exports": "./src/main.ts",
    "publish": {
        "include": [
            "./src/**/*",
            "README.md",
            "deno.json",
            "deno.lock",
            "LICENSE"
        ],
        "exclude": [
            "dist"
        ]
    },
    "tasks": {
        "dev": "deno run --watch ./src/main.ts"
    },
    "imports": {
        "@std/assert": "jsr:@std/assert@^1.0.11",
        "@std/testing": "jsr:@std/testing@^1.0.9",
        "@collidor/event": "jsr:@collidor/event@^4.0.6"
    },
    "lint": {
        "rules": {
            "exclude": [
                "no-explicit-any"
            ],
            "include": [
                "no-console"
            ]
        }
    }
}